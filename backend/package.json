{
  "name": "vision-grade-backend",
  "version": "1.0.0",
  "description": "Backend API for VisionGrade SPAS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test/test-runner.js all",
    "test:unit": "node test/test-runner.js unit",
    "test:integration": "node test/test-runner.js integration",
    "test:models": "node test/test-runner.js models",
    "test:performance": "node test/test-runner.js performance",
    "test:watch": "mocha --recursive test/ --watch",
    "test:coverage": "nyc mocha --recursive test/",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "docx": "^8.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.4",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.8",
    "sequelize": "^6.32.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "faker": "^5.5.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^6.6.1",
    "sinon": "^21.0.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.3.3"
  }
}
